<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.colgorithmic.isqtbquestionbank">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ExamQuestionBank"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SubCategory"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuestionTemaplate" />
        <activity android:name=".Instruction" />
        <activity android:name=".AllTestList" />
        <activity android:name=".TestPaper">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            </intent-filter>
        </activity>
        <activity
            android:name=".Results"
            android:screenOrientation="portrait" />
        <activity
            android:name=".choose_question"
            android:label="ISTQB-500" />
        <activity android:name=".about_us" />
        <activity
            android:name=".ShowQuestionResult"
            android:label="@string/title_activity_show_question_result"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BarCharts"
            android:label="@string/title_activity_bar_charts"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AboutTest"
            android:label="@string/title_activity_about_istqb"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ContactUs"
            android:label="@string/title_activity_contact_us"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AppAudience"
            android:label="@string/title_activity_app_audience"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ProductInfo"
            android:label="@string/title_activity_product_info"
            android:theme="@style/AppTheme.NoActionBar"></activity>


    </application>


</manifest>
